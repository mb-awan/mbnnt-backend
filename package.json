{
  "name": "mbnnt-backend",
  "version": "1.0.11",
  "description": "An Express boilerplate backend",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch --clear-screen=false src/index.ts | pino-pretty",
    "stage": "cross-env NODE_ENV=staging node dist/index.js",
    "build": "cross-env NODE_ENV=production rimraf dist && tsup ",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "seed:dev": "cross-env NODE_ENV=development tsx src/seeders/index.ts | pino-pretty",
    "seed:prod": "cross-env NODE_ENV=production tsx src/seeders/index.ts | pino-pretty",
    "seed:stage": "cross-env NODE_ENV=staging tsx src/seeders/index.ts | pino-pretty",
    "start-docker:dev": "docker compose -f docker-compose.dev.yml up",
    "install-dependencies-docker:dev": "docker exec -it dev-api-server npm ci",
    "seed-docker:dev": "docker exec -it dev-api-server npm run seed:dev",
    "stop-docker:dev": "docker compose -f docker-compose.dev.yml down",
    "start-docker:prod": "docker compose -f docker-compose.prod.yml up",
    "install-dependencies-docker:prod": "docker exec -it prod-api-server npm ci",
    "seed-docker:prod": "docker exec -it prod-api-server npm run seed:prod",
    "stop-docker:prod": "docker compose -f docker-compose.prod.yml down",
    "clean": "rimraf dist coverage",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --config .prettierrc --write .",
    "test": "cross-env NODE_ENV=test vitest run",
    "test:dev": "vitest dev",
    "test:cov": "vitest run --coverage",
    "release": "semantic-release",
    "prepare": "husky"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.0.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cloudinary": "^2.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.1",
    "multer": "^1.4.5-lts.1",
    "pino-http": "^10.0.0",
    "redis": "^4.7.0",
    "serverless-http": "^3.2.0",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "semantic-release": "^24.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.4",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "**/*": "npm run format",
    "**/*.{js,ts}": "npm run lint:fix"
  },
  "author": "Edwin Hernandez",
  "repository": "edwinhern/express-typescript-2024",
  "license": "ISC"
}
